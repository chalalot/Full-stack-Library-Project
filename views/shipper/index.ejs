  <body>
      <h1>Active Orders</h1>
      <div id="order-list">
        <ul>
          <% orders.forEach(function(order) { %>  
            <a href="shipper/<%= order._id %>">
              <h1><%= order.customer.name %></h1>
              <h1><%= order.address %></h1>
              <h1><%= order.orderDate.toLocaleDateString() %></h1>
              <h1><%= order.totalCost %></h1>
            </a>
          <% }); %> 
        </ul>
      </div>
  </body>
</html>


